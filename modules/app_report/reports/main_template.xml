<?xml version="1.0" encoding="utf-8"?>
<odoo>
<!--main template which calls "Tax Invoice" and "Tax Invoice-Copy" templates.-->
<template id="app_report.main_template">
  	<t t-call="web.html_container">  
 	    <t t-foreach="docs" t-as="o">
               <t t-call="app_report.report_tax_invoice" />
               <xpath expr="//t[@t-call='app_report.report_tax_invoice_copy']" position="replace">
               <div></div>
               </xpath>
               <t t-call="app_report.report_tax_invoice_copy" />
        </t>
    </t>
        
</template>	

</odoo>
